{
  "name": "laravel-fast-boot",
  "displayName": "Laravel Fast Boot",
  "description": "Bootstrap fresh Laravel projects with a guided setup wizard",
  "version": "1.0.4",
  "publisher": "MuhammadYusuf",
  "icon": "/images/icon.png",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/VernSG/laravel-fast-boot"
  },
  "keywords": [
    "laravel",
    "php",
    "setup",
    "bootstrap",
    "wizard",
    "artisan",
    "composer",
    "sail"
  ],
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "workspaceContains:**/composer.json"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "laravelFastBoot.setup",
        "title": "Laravel: Run First Setup",
        "category": "Laravel Fast Boot"
      },
      {
        "command": "laravelFastBoot.setupFromContext",
        "title": "Setup Laravel Project"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "laravelFastBoot.setupFromContext",
          "when": "resourceFilename == composer.json",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.107.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.48.1",
    "eslint": "^9.39.1",
    "esbuild": "^0.27.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
